{
  "main": "./dist/SassifyWebpackPlugin.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "prepublish": "node ./bin/build.js prepublish",
    "build": "node ./bin/build.js build",
    "clean": "node ./bin/build.js clean",
    "test": "tsc -p ./test/tsconfig.json && ava",
    "cover": "tsc --rootDir ./src --outDir ./src --sourcemap && nyc --reporter lcov --reporter text npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Jesse Linthorst <jesse.linthorst@gmail.com> (http://jesselinthorst.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesse-mm/sassify-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jesse-mm/sassify-webpack-plugin",
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/mkdirp": "^0.3.29",
    "@types/mustache": "^0.8.29",
    "@types/node": "^7.0.18",
    "@types/pathjs": "^0.8.32",
    "@types/pify": "^0.0.28",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "codecov.io": "^0.1.6",
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "nyc": "^10.3.2",
    "path": "^0.12.7",
    "pify": "^2.3.0",
    "semantic-release": "^6.3.6",
    "shelljs": "^0.7.7",
    "typescript": "^2.3.2",
    "typescript-compiler": "^1.4.1-2",
    "vm2": "^3.4.6",
    "webpack": "^2.5.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/mock/**/*"
    ],
    "source": [
      "src/*.js",
      "!dist/*"
    ],
    "babel": {
      "presets": [
        "es2015"
      ]
    },
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {}
}
