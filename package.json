{
  "name": "sassify-webpack-plugin",
  "main": "./dist/SassifyWebpackPlugin.js",
  "description": "A JavaScript/TypeScript object parser that generate scss maps/variables",
  "version": "1.0.8",
  "keywords": [
    "sass",
    "scss",
    "javascript",
    "js",
    "typescript",
    "ts",
    "webpack",
    "plugin"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "build": "node ./bin/build.js build",
    "clean": "node ./bin/build.js clean",
    "test": "tsc -p ./test/tsconfig.json && ava",
    "doc": "typedoc --out doc/typedoc/ src/",
    "cover": "tsc --rootDir ./src --outDir ./src --sourcemap && nyc --reporter lcov --reporter text npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "author": "Jesse Linthorst <jesse.linthorst@gmail.com> (http://jesselinthorst.nl)",
  "bugs": {
    "url": "https://github.com/jesse-mm/sassify-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jesse-mm/sassify-webpack-plugin",
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/mkdirp": "^0.3.29",
    "@types/mustache": "^0.8.29",
    "@types/node": "^7.0.18",
    "@types/pathjs": "^0.8.32",
    "@types/pify": "^0.0.28",
    "ava": "^0.19.1",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "codecov.io": "^0.1.6",
    "nyc": "^10.3.2",
    "path": "^0.12.7",
    "shelljs": "^0.7.7",
    "typedoc": "^0.7.1",
    "webpack": "^2.5.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/mock/**/*"
    ],
    "source": [
      "src/*.js",
      "!dist/*"
    ],
    "babel": {
      "presets": [
        "es2015"
      ]
    },
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "pify": "^2.3.0",
    "typescript": "^2.3.3",
    "vm2": "^3.4.6"
  }
}
